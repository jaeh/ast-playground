{
  "name": "@magic/core",
  "version": "0.0.1",
  "author": "Wizards & Witches",
  "description": "@magic core. generate static pages and serverless lambdas. ~5kb client boilerplate.",
  "license": "AGPL-3.0",
  "homepage": "https://magic.github.io/core",
  "scripts": {
    "start": "t -p",
    "build": "NODE_ENV=production node src/bin/bin.js build",
    "serve": "node src/bin/bin.js serve --watch src",
    "prod": "NODE_ENV=production node src/bin/bin.js build serve --watch src",
    "clean": "node src/bin/bin.js clean",
    "dev": "node src/bin/bin.js dev --watch src",
    "format": "f -w --exclude docs",
    "test": "t",
    "calls": "calls"
  },
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "bin": {
    "magic": "src/bin.mjs"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magic/core.git"
  },
  "bugs": {
    "url": "https://github.com/magic/core/issues"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/preset-env": "7.4.4",
    "@magic/cli": "0.0.2",
    "@magic/css": "0.7.0",
    "@magic/deep": "0.0.1",
    "@magic/log": "0.1.0",
    "@magic/types": "0.1.2",
    "babel-plugin-minify-builtins": "0.5.0",
    "babel-plugin-minify-dead-code-elimination": "0.5.0",
    "babel-plugin-minify-mangle-names": "0.5.0",
    "babel-plugin-minify-simplify": "0.5.0",
    "babel-plugin-minify-type-constructors": "0.4.3",
    "babel-plugin-remove-code": "0.0.6",
    "babel-plugin-transform-minify-booleans": "6.9.4",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-transform-remove-debugger": "6.9.4",
    "babelify": "10.0.0",
    "browserify": "16.2.3",
    "hyperapp": "1.2.9",
    "hyperapp-render": "3.1.0"
  },
  "devDependencies": {
    "@magic-modules/git-badges": "github:magic-modules/git-badges",
    "@magic-modules/library-list": "github:magic-modules/library-list",
    "@magic-modules/module-list": "github:magic-modules/module-list",
    "@magic-modules/pre": "github:magic-modules/pre",
    "@magic-modules/theme-list": "github:magic-modules/theme-list",
    "@magic-themes/docs": "github:magic-themes/docs",
    "@magic/test": "0.1.4"
  },
  "keywords": [
    "magic",
    "static",
    "page",
    "generator",
    "serverless"
  ],
